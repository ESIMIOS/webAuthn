{
	"name": "webauthn",
	"version": "0.0.10",
	"description": "Utilidades para el uso de WebAuthn",
	"main": "/lib/index.js",
	"types": "lib/index.d.ts",
	"scripts": {
		"test": "jest --config jestconfig.json",
		"build": "tsc",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "npx eslint \"src/**/*.{js,ts}\"",
		"prepare": "npm run build",
		"prepublishOnly": "npm test && npm run lint",
		"preversion": "npm run lint",
		"version": "npm run format && git add -A src",
		"postversion": "git push && git push --tags"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ESIMIOS/webAuthn.git"
	},
	"keywords": [
		"webauthn",
		"fido2"
	],
	"author": "ESIMIOS",
	"bugs": {
		"url": "https://github.com/ESIMIOS/webAuthn/issues"
	},
	"homepage": "https://github.com/ESIMIOS/webAuthn#readme",
	"devDependencies": {
		"@eslint/eslintrc": "3.1.0",
		"@eslint/js": "9.12.0",
		"@types/jest": "29.5.13",
		"@typescript-eslint/eslint-plugin": "8.10.0",
		"@typescript-eslint/parser": "8.10.0",
		"eslint": "9.12.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-prettier": "5.2.1",
		"jest": "29.7.0",
		"prettier": "3.3.3",
		"ts-jest": "29.2.5",
		"tslint-config-prettier": "1.18.0",
		"typescript": "5.2.2"
	},
	"dependencies": {
		"byte-base64": "1.1.0",
		"cbor-redux": "0.4.0",
		"elliptic": "6.5.7",
		"jest-environment-jsdom": "29.7.0",
		"js-sha256": "0.11.0"
	}
