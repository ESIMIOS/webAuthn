{
    "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "testEnvironment": "jsdom",
    "collectCoverage": true,
    "collectCoverageFrom": ["./src/**/*"],
    "coverageDirectory": "./__coverage__",
    "coverageThreshold": {
    "global": {
        "branches": 80,
        "functions": 100,
        "lines": 90,
        "statements": 90
    }
    }

}
